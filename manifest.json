{
  "name": "Find scientific words",
  "description": "its the best",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["styles.css", "src/main.css"],
      "js": ["jquery.js", "highlight.js", 
      "src/tooltip.js", "src/queryHighlightedWord.js"]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
    "*"
  ]
}
