{
  "name": "Find scientific words",
  "description": "its the best",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [
        "quill.css",
        "src/main.css"
      ],
      "js": [
        "mark.js",
        "jquery.js",
        "highlight.js", 
        "src/tooltip.js",
        "src/queryHighlightedWord.js",
        "src/highlightWord.js",
        "quill.js"
      ] 
    }
  ],
  "icons": {
    "16": "icon.png"
  },
  "browser_action": {
      "default_icon": "spacetionary_logo.png"
  },
  "manifest_version": 2,
  "web_accessible_resources": [
    "*"
  ]
}
